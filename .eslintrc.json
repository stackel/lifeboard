{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "gapi": true,
    "document": true,
    "window": true,
    "cast": true,
    "chrome": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }]
  }
}